generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String  @id @default(cuid())
  language String?
  email    String  @unique
  password String?
  name     String?
  image    String?
  provider String?
  created_at DateTime @default(now())
}